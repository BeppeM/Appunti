
public class Mappa implements ISupervisore {

	private int maxX;
	private int minX;
	private int maxY;
	private int minY;
	private PuntoXY obbiettivo;
	
	public Mappa(int maxX, int minX, int maxY, int minY, PuntoXY obbiettivo) {
		if(minX>maxX || minY>maxY || obbiettivo==null || obbiettivo.getX()>maxX||obbiettivo.getX()<minX || obbiettivo.getY()>maxY||obbiettivo.getY()<minY )
		this.maxX = maxX;
		this.minX = minX;
		this.maxY = maxY;
		this.minY = minY;
		this.obbiettivo = obbiettivo;
	}

	@Override
	public boolean posizioneValida(PuntoXY posizione) {
		// TODO Auto-generated method stub
		return false;
	}

	@Override
	public boolean obiettivoRaggiunto(PuntoXY posizione) {
		// TODO Auto-generated method stub
		return false;
	}

}
