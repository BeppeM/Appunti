import java.util.List;

public class VoloMultiTratta extends Volo {
	private List<VoloDiretto> tratte;
	
	private  String estraiAreoPortoDiPartenza(List<VoloDiretto> tratte) {
		if(tratte!=null && tratte.size()>2)
			return tratte.get(0).getAeroportoPartenza();
	}

	
	public VoloMultiTratta(String aeroportoPartenza, String aeroportoArrivo, List<VoloDiretto> tratte) {
		super(aeroportoPartenza, aeroportoArrivo);
		this.tratte = tratte;
		
	}
	
	

	@Override
	public int getNumeroTratte() {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public int getDurataInMinuti() {
		// TODO Auto-generated method stub
		return 0;
	}

}
